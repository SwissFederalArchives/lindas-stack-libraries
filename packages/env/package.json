{
  "name": "@lindas/env",
  "version": "3.0.5",
  "description": "RDF/JS Environment",
  "type": "module",
  "main": "index.js",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "default": "./index.js"
    },
    "./web": {
      "types": "./web.d.ts",
      "default": "./web.js"
    },
    "./web.js": {
      "types": "./web.d.ts",
      "default": "./web.js"
    },
    "./Environment": {
      "types": "./Environment.d.ts",
      "default": "./Environment.js"
    },
    "./Environment.js": {
      "types": "./Environment.d.ts",
      "default": "./Environment.js"
    },
    "./formats": {
      "types": "./formats.d.ts",
      "default": "./formats.js"
    },
    "./lib/*": "./lib/*.js"
  },
  "scripts": {
    "prepack": "tsc",
    "test": "mocha --package ../../package.json",
    "build": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SwissFederalArchives/lindas-stack-libraries.git",
    "directory": "packages/env"
  },
  "bugs": {
    "url": "https://github.com/zazuko/env/issues"
  },
  "files": [
    "*.js",
    "*.d.ts",
    "lib/*.js",
    "lib/*.d.ts",
    "CHANGELOG.md"
  ],
  "keywords": [
    "rdfjs",
    "environment"
  ],
  "author": "Zazuko GmbH",
  "license": "MIT",
  "dependencies": {
    "@rdfjs/dataset": "^2.0.1",
    "@rdfjs/data-model": "^2.0.1",
    "@rdfjs/formats": "^4.0.0",
    "@rdfjs/namespace": "^2.0.0",
    "@rdfjs/term-map": "^2.0.0",
    "@rdfjs/term-set": "^2.0.1",
    "@rdfjs/traverser": "^0.1.2",
    "@tpluscode/rdf-ns-builders": "^5",
    "@lindas/env-core": "*",
    "@lindas/prefixes": "*",
    "clownface": "^2.0.2",
    "get-stream": "^8.0.0",
    "rdf-dataset-ext": "^1.1.0",
    "readable-stream": "^4.5.2",
    "util": "^0.12.5"
  },
  "peerDependencies": {
    "@rdfjs/types": "^2",
    "@types/clownface": "^2.0.0",
    "@types/rdf-dataset-ext": "^1.0.8",
    "@types/rdfjs__dataset": "^2.0.7",
    "@types/rdfjs__data-model": "^2.0.9",
    "@types/rdfjs__environment": "^1.0.0",
    "@types/rdfjs__namespace": "^2.0.10",
    "@types/rdfjs__term-map": "^2.0.10",
    "@types/rdfjs__term-set": "^2.0.9",
    "@types/rdfjs__traverser": "^0.1.5",
    "@types/rdfjs__formats": "^4.0.1"
  },
  "devDependencies": {
    "@rdfjs-elements/formats-pretty": "^0.6.7",
    "@types/chai": "^4.3.5",
    "@types/mocha": "^10.0.1",
    "chai": "^4.3.7",
    "mocha": "^10.2.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
