{
  "name": "@lindas/formats-lazy",
  "version": "4.7.0",
  "description": "Parsers and serializers for common RDF formats, loaded lazily",
  "type": "module",
  "main": "index.js",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "default": "./index.js"
    }
  },
  "files": [
    "*.js",
    "*.d.ts",
    "lib/*.js",
    "lib/*.d.ts",
    "CHANGELOG.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/SwissFederalArchives/lindas-stack-libraries.git",
    "directory": "packages/formats-lazy"
  },
  "keywords": [
    "rdf",
    "rdfjs",
    "format",
    "parser",
    "serializer"
  ],
  "author": "Zazuko GmbH",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SwissFederalArchives/lindas-stack-libraries/issues"
  },
  "dependencies": {
    "@rdfjs/parser-jsonld": "^2.1.3",
    "@rdfjs/parser-n3": "^2.1.0",
    "@rdfjs/serializer-jsonld": "^2.0.1",
    "@rdfjs/serializer-ntriples": "^2.0.1",
    "@rdfjs/sink-map": "^2.0.1",
    "@types/rdfjs__parser-jsonld": "^2.1.7",
    "@types/rdfjs__serializer-jsonld": "^2.0.5",
    "@types/rdfjs__parser-n3": "^2.0.6",
    "@types/rdfjs__serializer-ntriples": "^2.0.6",
    "@types/rdfjs__sink-map": "^2.0.5",
    "onetime": "^6.0.0",
    "rdfxml-streaming-parser": "^3.0.1",
    "readable-stream": "^4"
  },
  "publishConfig": {
    "access": "public"
  }
}
