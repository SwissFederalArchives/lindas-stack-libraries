{
  "name": "lindas-stack-libraries",
  "version": "0.0.4",
  "private": true,
  "description": "LINDAS RDF Stack - Monorepo for all LINDAS RDF libraries",
  "repository": {
    "type": "git",
    "url": "https://github.com/SwissFederalArchives/lindas-stack-libraries.git"
  },
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "test": "turbo run test",
    "lint": "turbo run lint",
    "clean": "turbo run clean",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "release": "turbo run build test && changeset publish"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.0",
    "ts-node": "^10.9.2",
    "turbo": "^2.3.0",
    "typescript": "^5.3.0"
  },
  "overrides": {
    "braces": "^3.0.3",
    "micromatch": "^4.0.8",
    "cross-spawn": "^7.0.6",
    "js-yaml": "^4.1.1",
    "nanoid": "^3.3.8",
    "serialize-javascript": "^6.0.2",
    "postcss": "^8.4.31",
    "axios": "^1.7.9",
    "esbuild": "^0.25.0",
    "@zazuko/rdf-parser-csvw-xlsx": "npm:@lindas/rdf-parser-csvw-xlsx@^1.0.1"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "npm@10.0.0"
}
